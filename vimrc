" General_conf{
    set nocompatible
    scriptencoding utf-8
    filetype off
    
    set number
    set linebreak
    set textwidth=100
    set showmatch
    set ruler
    set visualbell

    set autoindent
    set smarttab
    set tabstop=4
    set softtabstop=4
    set shiftwidth=4
    set expandtab

    "set cursorline
    "set cursorcolumn
    "set colorcolumn=80
    
    set autowrite

"}
